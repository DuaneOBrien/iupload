<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0" >

    <display-name>ImhotepUploader</display-name>

    <!-- Configured in com.indeed.iupload.WebApp -->

    <!-- This forces Tomcat to only look for initializers in spring-web package instead of all the jars.
        Saves a lot of time on reload -->
    <absolute-ordering>
        <name>spring_web</name>
        <name>iupload_webapp</name>
    </absolute-ordering>
</web-app>
